def palindrome(string):
    
    reversed_string = []
    i = -1
    for letter in string:
        reversed_string.append(string[i])
        i += -1
        
    pointer = 0

    
    for letter_1, letter_2 in zip(string, reversed_string):
        if letter_1 == letter_2:
            return print(True)
        else:
            return print(False)
